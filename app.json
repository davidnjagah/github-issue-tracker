{
  "expo": {
    "name": "github-issue-tracker",
    "slug": "github-issue-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icon_git_1020x1020.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/git_Splash_Screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#00ffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Icon_git_1020x1020.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
